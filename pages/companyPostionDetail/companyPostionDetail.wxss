@import '../template/common';

#wrap {
  background: #f8f8f8;
  font-size: 26rpx;
  width: 100%;
  min-height: 100%;
}

/* SRARCH INFO START */

.inner {}

.yuangongfuliguanli {
  width: 34rpx;
  height: 34rpx;
  margin-right: 16rpx;
}

.zhengshuicon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
}

.search-inner {
  margin: 25rpx 0 10rpx;
  display: flex;
  align-items: center;
  line-height: 48rpx;
}

.s-left-arrow {
  flex: 0 0 1;
  width: 40rpx;
  height: 40rpx;
  margin-top: 15rpx;
}

.s-left-arrow .lef-icon {
  width: 16rpx;
  height: 25rpx;
}

.search-cont {
  margin: 0 24rpx 0 24rpx;
  flex: 0 0 1;
  width: 80%;
  box-sizing: border-box;
  position: relative;
}

.search {
  width: 100%;
  border-radius: 28px;
  height: 60rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
  display: inline-block;
  font-size: 24rpx;
  position: relative;
}

.search_icon {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  left: 54rpx;
  top: 30rpx;
}

.collect_tab {}

