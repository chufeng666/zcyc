<!-- 头部 -->
<view class='i-header'>
  <view class="h-left">
    <navigator url="../index/index" class="iconfont icon-zuojiantou"></navigator>
    <view class="l_qiye">招人</view>
  </view>
  <view class="h-bottom">
    <image class="search-icon" src='../../../static/images/public/search_icon.png'></image>
    <input class=' search-nosearch'  bindinput="handeSearchInput" placeholder="一级建造师"></input>
  </view>
</view>
<!-- 头部 -->
<!-- 筛选 -->
<view class="newest-search">
  <view class="n-xiahuaxian">
    <view class="newest">
      <view class="province" wx:if="{{showTST}}">
        <text decode="{{true}}">广州市 - 海珠区&nbsp;&nbsp;|&nbsp;&nbsp;</text>
        <picker mode="selector" range="{{jobArray}}" bindchange="setJobsearch">
           {{job_intention}}
        </picker>
      </view>
      <view class="province" wx:else>
        <text decode="{{true}}">{{areaInfo}}&nbsp;&nbsp;|&nbsp;&nbsp;</text>
        <picker mode="selector" range="{{jobArray}}" bindchange="setJobsearch">
           {{job_intention}}
        </picker>
      </view>
    </view>
    <view class="n-search">
      <view class="newsite_button active" bindtap="selectDistrict">
        <view class="newsite_view subtitle-msg  active">地点</view>
        <icon class="iconfont icon-zhcc_xiangxiajiantou"></icon>
      </view>
      <navigator class="newsite_button  active" url="../ScreeningConditions/ScreeningConditions">
        <view class="newsite_view subtitle-msg  ">更多</view>
        <icon class="iconfont icon-zhcc_xiangxiajiantou"></icon>
      </navigator>
    </view>
  </view>
</view>
<!-- 筛选 -->
<!-- 数据展示 -->
<view class="inner">
  <view class="container">
    <import src="../../template/companyJobSearch/companyJobSearch" />
    <template is="JobSearch" data="{{recList:hiringData,url:'../../cOrTInfo/cOrTInfoDetail?id='}}" />
  </view>
</view>
<!-- 所有城市三级联动列表 -->
<address-form id='address' bind:tabEvent="tabEvent"></address-form>
<import src="../../template/tabbar.wxml" />
<template is="tabBar" data="{{tabBar}}" />