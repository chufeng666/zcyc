.i-header {
  height: 208rpx;
  background-color: var(--thirdPartyColor);
  line-height: 104rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}

.i-header .h-left {
  color: #fff;
  font-weight: 500;
  font-size: 40rpx;
}

.i-header .h-bottom {
  padding: 24rpx 32rpx;
}

.i-header .h-bottom .search-cont {
  display: flex;
  align-items: center;
  height: 56rpx;
  background-color: #fff;
  border-radius: 36px;
}

.i-header .h-bottom .search-cont .search {
  width: 100%;
  border-radius: 36px;
  height: 60rpx;
  padding-left: 55rpx;
  box-sizing: border-box;
  font-size: 24rpx;
}

.i-header .h-bottom .search-cont .search-nosearch {
  font-size: 24rpx;
  color: #999;
  width: 100%;
}

.i-header .h-bottom .search-cont .search-icon {
  width: 32rpx;
  height: 32rpx;
  margin: 0 16rpx;
}

.i-header .new {
  /* border-bottom: 16rpx solid #dedede; */
}

.i-header .new .new_xiaoxi {
  display: flex;
  align-content: center;
  background-color: #ffeeab;
  height: 74rpx;
  line-height: 74rpx;
}

.i-header .new .new_xiaoxi .n-lfet {
  text-align: center;
  margin-left: 34rpx;
  font-size: 32rpx;
  font-weight: 700;
  color: #f0a930;
  display: flex;
}

.i-header .new .new_xiaoxi .n-lfet view {
  font-weight: 400;
  margin-left: 8rpx;
}

.i-header .new .new_xiaoxi .n-right {
  font-size: 28rpx;
  font-weight: 500;
  color: #686868;
  margin-left: 34rpx;
}

.newest-search {
  background-color: #fff;
  text-align: center;
  margin-top: 282rpx;
  padding: 0 30rpx;
  border-top: 16rpx solid #dedede;
}

.newest-search .n-xiahuaxian {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 1px solid #dedede;
}

.newest-search .n-xiahuaxian .newest {
  display: flex;
  font-size: 30rpx;
  font-weight: 400;
  color: #686868;
}

.newest-search .n-xiahuaxian .newest .lively:nth-child(2) {
  margin-left: 16rpx;
}

.newest-search .n-xiahuaxian .newest .active {
  font-family: PingFang SC;
  font-weight: bold;
  color: #262626;
}

.newest-search .n-xiahuaxian .n-search {
  display: flex;
  font-size: 28rpx;
  color: #969696;
}

.newest-search .n-xiahuaxian .n-search .newsite_button:first-child {
  display: flex;
  align-items: center;
  background-color: #eaeaea;
  margin-right: 16rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 8%;
}
.newest-search .n-xiahuaxian .n-search .newsite_button {
  display: flex;
  align-items: center;
  background-color: #eaeaea;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 8%;
}
.newest-search .n-xiahuaxian .n-search .newsite_button .newsite_view {
  margin: 0 10rpx;
}

.newest-search .n-xiahuaxian .n-search .newsite_button .newsite_city {
  padding:0 10rpx;
}

.newest-search .n-xiahuaxian .n-search .newsite_button .icon-zhcc_xiangxiajiantou {
  font-size: 16rpx;
  margin-right: 16rpx;
}

.newest-search .n-xiahuaxian .n-search picker .subtitle-msg {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  background-color: #eaeaea;
  height: 56rpx;
  border-radius: 8%;
  padding-left: 10rpx;
}

.newest-search .n-xiahuaxian .n-search picker .subtitle-msg .icon-zhcc_xiangxiajiantou {
  font-size: 25rpx;
  margin: 0 16rpx;
}

.n-information {
  margin-bottom: 95rpx;
}

.n-information .c_info {
  padding: 32rpx 30rpx;
  border-bottom: 10px solid #dedede;
}

.n-information .c_info .i_list {
  display: flex;
  justify-content: space-between;
  align-content: center;
}

.n-information .c_info .i_list .l_yuan {
  font-size: 34rpx;
  font-weight: 550;
  color: #000;
}

.n-information .c_info .i_list .l_price {
  color: var(--thirdPartyColor);
  font-weight: 600;
  font-size: 32rpx;
}

.n-information .c_info .i_second {
  display: flex;
  margin-top: 14rpx;
}

.n-information .c_info .i_second view {
  color: #fff;
  font-size: 24rpx;
  font-weight: normal;
  background-color: var(--thirdPartyColor);
  padding: 6rpx 15rpx;
  border-radius: 4rpx;
}

.n-information .c_info .i_second view:nth-child(2) {
  margin-left: 15rpx;
}

.n-information .c_info .i_third {
  color: #686868;
  font-size: 26rpx;
  display: flex;
  margin-top: 10rpx;
}

.n-information .c_info .i_third .t_zhili {
  margin: 0 28rpx;
}

.n-information .c_info .i_fourth {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  margin-top: 11rpx;
}

.n-information .c_info .i_fourth .f_logo {
  background-color: goldenrod;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
}

.n-information .c_info .i_fourth .f_zhiwei {
  color: #262626;
  margin-left: 20rpx;
}

.n-information .c_info .i_fourth .f_gongsi {
  color: #969696;
  margin-left: 30rpx;
}

.active {
  color: var(--thirdPartyColor);
  font-weight: 400;
}

.picker-view {
  width: 100%;
  display: flex;
  z-index: 12;
  background-color: #fff;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  height: 40vh;
}

.picker-item {
  line-height: 70rpx;
  margin-left: 5rpx;
  margin-right: 5rpx;
  text-align: center;
}

.zhezhao {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  z-index: 9;
}

.sousuolishi {
  margin-top: 308rpx;
  border-bottom: 16rpx solid #dedede;
}

.sousuolishi .history {
  border-bottom: 10px solid #dedede;
}

.sousuolishi .history .history_search {
  padding: 0 34rpx;
}

.sousuolishi .history .history_search .hs_top {
  display: flex;
  justify-content: space-between;
  margin-bottom: 47rpx;
}

.sousuolishi .history .history_search .hs_top .t_left {
  font-size: 30rpx;
  font-weight: 600;
}

.sousuolishi .history .history_search .hs_content {
  display: flex;
  color: #686868;
  margin-bottom: 47rpx;
}

.sousuolishi .history .history_search .hs_content .con_zhiye {
  margin-left: 16rpx;
  font-size: 26rpx;
}

.sousuolishi .popular {
  margin: 27rpx 33rpx;
}

.sousuolishi .popular .popular_search {
  font-size: 30rpx;
  font-weight: 600;
  margin-bottom: 47rpx;
}

.sousuolishi .popular .p_zhiwei {
  display: flex;
}

.sousuolishi .popular .p_zhiwei .zw_security-officer {
  background-color: var(--thirdPartyColor);
  padding: 22rpx 35rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 50rpx;
}

.sousuolishi .popular .p_zhiwei .zw_executive-secretary {
  background-color: #fda531;
  padding: 22rpx 35rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 50rpx;
  margin: 0 24rpx;
}

@import "../../template/font";