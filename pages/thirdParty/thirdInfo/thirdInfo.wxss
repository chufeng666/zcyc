page {
  background-color: #e0d9d7;
}

/* <!-- 头部 --> */

.i-header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  background-color: var(--thirdPartyColor);
  height: 127rpx;
  font-size: 36rpx;
  color: #fff;
  line-height: 163rpx;
  padding: 0 15rpx;
}

.icon-zuojiantou {
  margin-top: 4.5rpx;
  font-size: 48rpx;
}

/* <!-- 头部 --> */

/* 主体 */

#wrap {
  background-color: #fff;
  margin-top: 127rpx;
  margin-bottom: 200rpx;
}

/* <!-- TITLE START --> */

.content-list {
}

.main_top {
  padding: 24rpx;
}

.c-l-info {
  display: flex;
}

.c-logo {
  width: 142rpx;
  height: 142rpx;
  border-radius: 50%;
}

.c-info-text {
  flex: 2;
  margin-left: 40rpx;
}

.c-info-title {
  display: flex;
  align-items: center;
  margin-bottom: 39rpx;
}

.info_name {
  font-size: 40rpx;
  color: #303133;
  margin-right: 24rpx;
}

.info_huiyuan {
  width: 84rpx;
  height: 37rpx;
}

.m-b10 {
  font-size: 24rpx;
  color: #999;
}

.main_top .c-l-info .u-o-list {
  display: flex;
  font-size: 22rpx;
  color: #999;
  align-items: flex-end;
}

.info_huiyuan .u-o-list view {
  margin-bottom: 5rpx;
}

.icon-youjiantou {
  font-size: 30rpx;
}

/* <!-- TITLE START --> */

/* 考试系统 */

.system {
  background-color: #deeffe;
  border-bottom: 16rpx solid #dedede;
  height: 128rpx;
  line-height: 128rpx;
}

.sy-aq_kaoshi {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 32rpx;
}

.sy_anquan {
  display: flex;
  align-items: center;
  color: #686868;
}

.icon-baobao {
  color: var(--thirdPartyColor);
  margin-right: 22rpx;
}

.sy_aq_large {
  font-size: 40rpx;
  margin-bottom: 14rpx;
}

.sy_aq_smart {
  font-size: 30rpx;
}

.sy_ceshi {
  width: 80rpx;
  height: 80rpx;
  background-color: var(--thirdPartyColor);
  border-radius: 50%;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
}

.sy_ceshi .icon-hao {
}

.c-l-info .u-o-list {
  margin-bottom: 24rpx;
}

.u-o-list {
  display: flex;
}

.user_img {
  width: 36rpx;
  height: 36rpx;
  padding: 29rpx 0;
  margin-right: 22rpx;
}

/* 考试系统 */

/* <!-- USER OPTION START --> */

.user-option {
  font-size: 30rpx;
  color: #686868;
  font-weight: 500;
}

.youmihuobi {
  border-bottom: 16rpx solid #dedede;
}

.user_caozuo {
  border-bottom: 16rpx solid #dedede;
}

.u-o-qianbao {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 26rpx 32rpx;
}

.user_price {
  display: flex;
}

.usp_yuer {
  color: var(--themeColor);
}

.user_price .icon-youjiantou {
  margin-top: 4rpx;
}

.uscz_neirong, .user_qiehuan {
  font-size: 28rpx;
  padding: 0 32rpx;
}

.uscz_neirong .u-o-list, .user_qiehuan .u-o-list {
 
}

.uscz_neirong .u-o-list .u-o_left, .user_qiehuan .u-o-list .u-o_left {
  display: flex;
  flex: 9;
  justify-content: space-between;
  padding: 29rpx 0;
  border-bottom: 1rpx solid #dedede;
}

.u-o-qianbao  .user_img {
  width: 36rpx;
  height: 36rpx;
  padding: 0;
  margin-right: 22rpx;
}

.uscz_neirong .u-o-list:last-child .u-o_left {
  border: none;
  margin: 0;
}

.user_qiehuan .u-o-list:last-child .u-o_left {
  font-size: 30rpx;
  color: #686868;
  border: none;
  margin: 0;
}

/* <!-- USER OPTION START --> */

/* 主体 */
@import '../../template/font';
