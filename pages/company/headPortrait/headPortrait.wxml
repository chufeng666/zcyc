<view id='header-pic' class='header-pic'>
  <view class='-pic-subtitle'>
    <text>上传头像</text>
  </view>
  <view class='id-card-top'>
    <view class='-add-img-'>
      <image wx:if="{{logo==null}}" class='addPic' bindtap='addPosPic' src='../../../static/images/public/plus.png' />
