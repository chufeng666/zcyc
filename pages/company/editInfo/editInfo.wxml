<view class="user_header">
  <view class="liaojie">请完善公司信息</view>
  <text class="iconfont  .icon-cha"></text>
</view>
<!-- pages/userInfo/editInfo.wxml -->
<view class='inner'>
  <view class='edit-info'>
    <view class='edit-cont'>
      <view class="ec_logo">
        <view class="touxiang">
          <view>头像</view>
          <view class="tx_jianyi">
            <text class="iconfont  icon-iconcopy"></text>
            <view>建议使用真实头像</view>
          </view>
        </view>
        <image bindtap="openActionsheet" class="por_img" src="{{logo||icCardPic.src}}" />
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view class="ec_top">
            <view>联系人</view>
            <view class="yiwanshan">已完善</view>
          </view>
          <navigator url="" class="ec_bt">
            <view>建议使用真实头像</view>
          </navigator>
        </view>
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view class="jibenxinxi">
            <view>公司基本信息</view>
            <view class="buwanzheng">[ 公司信息不完整 ]</view>
          </view>
          <view class="ec_bt">
            <view>建议使用真实头像</view>
          </view>
        </view>
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view class="ec_top">
            <view>行业标签</view>
            <view class="yiwanshan">已完善</view>
          </view>
          <view class="ec_bt">
            <view>建议使用真实头像</view>
          </view>
        </view>
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view class="ec_top">
            <view>公司资质</view>
            <view class="yiwanshan">已完善</view>
          </view>
          <view class="ec_bt">
            <view>建议使用真实头像</view>
          </view>
        </view>
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view>公司照片</view>
          <view class="ec_bt">
            <view>建议使用真实头像</view>
          </view>
        </view>
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view>营业执照</view>
          <view class="ec_bt">
            <view>建议使用真实头像</view>
          </view>
        </view>
      </view>
      <view class="ec_lianxiren">
        <view class="touxiang">
          <view>公司地址</view>
          <view class="ec_bt">
            <view>建议使用真实头像</view>
          </view>
        </view>
      </view>
      <view>
        <view class='set-option-1'>
          <button class='s-1-option company-bg-' bindtap='saveEditInfo'>保存</button>
        </view>
      </view>
    </view>
  </view>
  <!-- footer -->
</view>