.header {
  display: flex;
  align-items: center;
  background-color: #333233;
  padding: 66rpx 20rpx 24rpx 20rpx;
  color: #fff;
  font-size: 36rpx;
}

.icon-zuojiantou {
  font-size: 54rpx;
  margin-top: 5rpx;
}

.inner .main .main_first {
  padding: 34rpx 56rpx;
}

.inner .main .main_first .mf_top {
  display: flex;
  align-items: center;
  color: #b0b0b0;
  font-size: 26rpx;
  margin-bottom: 36rpx;
}

.inner .main .main_first .mf_top .lianxi {
  display: flex;
  align-items: center;
  color: #ff730e;
}

.inner .main .main_first .mf_top .lianxi .icon-iconcopy {
  margin-right: 8rpx;
}

.inner .main .main_first .mf_middle {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 24rpx;
}

.inner .main .main_first .mf_middle .mf_m_left {
  display: flex;
  align-items: center;
  font-size: 36rpx;
  color: #686868;
}

.inner .main .main_first .mf_middle .mf_m_left .mf_m_l_img {
  width: 44rpx;
  height: 44rpx;
  background-color: greenyellow;
  margin-right: 16rpx;
  border-radius: 50%;
}

.inner .main .main_first .mf_bt {
  padding: 23rpx 0;
  border-bottom: 1rpx solid #dedede;
  border-top: 1rpx solid #dedede;
  font-size: 26rpx;
  color: var(--thirdPartyColor);
}

.inner .main .main_first .mf_bt .mf_bt_top {
  display: flex;
  flex-direction: column;
}

.inner .main .main_first .mf_bt .mf_bt_top .mf_bt_top_left {
  display: flex;
  align-items: center;
}

.inner .main .main_first .mf_bt .mf_bt_top .mf_bt_top_left .icon-yanzhengma {
  font-size: 38rpx;
  margin-right: 16rpx;
}

.inner .main .main_seconds {
  padding: 56rpx 40rpx;
}

.inner .main .main_seconds .ms_top {
  font-size: 30rpx;
  color: #686868;
}

.inner .main .main_seconds .ms_middle {
  padding: 45rpx 0;
  border-bottom: 1rpx solid #dedede;
  font-size: 52rpx;
}

.inner .main .main_seconds .ms_bt {
  display: flex;
  align-items: center;
  margin-top: 41rpx;
  font-size: 30rpx;
  color: #b0b0b0;
}

.inner .main .main_seconds .ms_bt .ms_bt_img {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  margin-right: 17rpx;
}

.inner .main .main_seconds .ms_footer {
  margin-top: 294rpx;
}

.inner .main .main_seconds .ms_footer button.ms_ft_but {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #e8b05f;
  color: #fff;
}

@import "../../template/font";