<!-- 头部 -->
<view class='i-header'>
  <text bindtap="detil" class="iconfont  icon-zuojiantou"></text>
  <view class="h-left">公司联系人</view>
</view>
<view class="inner">
  <view class="header">
    <view class="top">
      <view>公司联系人</view>
      <view class="wanshan">
        <text class="iconfont  .icon-iconcopy"></text>
        <view>请完善联系人信息</view>
      </view>
    </view>
    <view class="main">
      <view class="m_top">联系人名称</view>
      <input bindinput="inputContactsName" placeholder="请输入姓名" value="{{contacts}}" />
    </view>
    <view class="main">
      <view class="m_top">联系人手机号码</view>
      <input bindinput="inputContactsMobile" placeholder="请输入联系人手机号码" value="{{mobile}}" />
    </view>
  </view>
  <view class="footer">
    <button bindtap="setCompanyInfoTwo">确定</button>
  </view>
</view>