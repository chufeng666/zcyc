@import "/pages/template/font";
@import "/pages/template/common1/common1";
@import "/pages/template/common";


.userId_main .userId_img {
  margin-bottom: 0;
}

.userId_shangchuang {
  display: flex;
  justify-content: center;
  margin-top: 30rpx;
  background-color: #dedede;
  height: 120rpx;
  line-height: 120rpx;
  border-radius: 10rpx;
  font-size: 26rpx;
  opacity: 0.8;
  margin-bottom: 200rpx;
}
.user_header {
  display: flex;
  align-items: center;
  height: 56rpx;
  background-color: #ffe4cc;
  color: #ff730e;
  margin-top: 180rpx;
}

.user_header .liaojie {
  flex: 18;
  text-align: center;
  font-size: 24rpx;
}

.user_header .icon-cha {
  flex: 1;
  font-size: 24rpx;
}
.icon-hao {
  font-weight: bold;
  margin-right: 15rpx;
}


.del {
  width: 178rpx;
  height: 72rpx;
  border-radius: 36rpx;
  line-height: 72rpx;
  background-color: #dedede;
  color: #1487F1;
  margin-right: 12rpx;
  font-size: 30rpx;
}

.add {
  width: 442rpx;
  height: 72rpx;
  background-color: var(--themeColor);
  color: #fff;
  border-radius: 36rpx;
  line-height: 72rpx;
  font-size: 30rpx;
}
.inpuTitle {
  margin-top: 30rpx;
  font-weight: normal;
  color: #262626;
}
.inpuTitle-real{
  margin-top: 30rpx;
  color: #262626;
}
.info_bt[disabled] {
  background-color: var(--themeColor) !important;
  color: #fff !important;
}
@import "userId";